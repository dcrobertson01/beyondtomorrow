{% extends 'partials/base.html.twig' %}


{% block content %}
<div id="body-inner">
    <h1>{{ page.title }}</h1>
    <p>
    	{{ page.content|raw }}
    </p>
    <p>
		{{ page.header.source.name}}
	</p>
    <p>
		{{ page.header.source.url }}
	</p>
    <p>
		{{page.header.source.desc }}
	</p>
    {{ page.header.source.name }}
    {% for author in page.header.authors %}
		{{ author.name}}
	{% endfor %}

	{% for link in page.header.links %}

		{{ link.title }}
		{{ link.site }}
		{{ link.url }}

	{% endfor %}
    <p>
        {% include 'partials/newsletter_subscribe_form.html.twig' %}
    </p>
</div>

{% endblock %}
